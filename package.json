{
  "name": "sushiswap-router",
  "version": "0.0.16",
  "description": "Sushi Router",
  "keywords": [
    "sushi",
    "routers"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/router"
  },
  "license": "UNLICENSED",
  "author": "Rouzwelt <rouzweltt@gmail.com>",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "bb": "tsc",
    "build": "npm run pack && npm run typings",
    "typings": "tsc",
    "pack": "esbuild ./src/index.ts --bundle --outfile=./dist/index.js --external:viem --external:memoize-fs --external:sushi --format=cjs --platform=node --target=es2021",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "dependencies": {
    "@ethersproject/abi": "5.7.0",
    "@ethersproject/address": "5.7.0",
    "@ethersproject/solidity": "5.7.0",
    "@uniswap/token-lists": "1.0.0-beta.33",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-periphery": "1.4.4",
    "big.js": "6.1.1",
    "date-fns": "3.3.1",
    "decimal.js-light": "2.5.1",
    "lodash.flatmap": "4.5.0",
    "memoize-fs": "github:rouzwelt/memoize-fs#e5fcc9f6effc4ad087514372a53a49d380520ad5",
    "numeral": "2.0.6",
    "seedrandom": "3.0.5",
    "serialijse": "0.3.0",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "toformat": "2.0.0",
    "viem": "2.8.13",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/big.js": "6.2.0",
    "@types/lodash.flatmap": "4.5.9",
    "@types/numeral": "2.0.3",
    "@types/seedrandom": "3.0.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@wagmi/core": "2.6.9",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  }
}
