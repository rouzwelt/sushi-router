{
  "name": "sushiswap-router",
  "version": "0.0.13",
  "description": "Sushi Router",
  "keywords": [
    "sushi",
    "routers"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/router"
  },
  "license": "UNLICENSED",
  "author": "Rouzwelt <rouzweltt@gmail.com>",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "npm run pack && npm run typings",
    "typings": "tsc",
    "pack": "esbuild ./src/index.ts --bundle --outfile=./dist/index.js --external:viem --format=cjs --platform=node --target=es2021",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "tsc -w",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "viem": "=1.6.0"
  },
  "devDependencies": {
    "@ethersproject/abi": "=5.7.0",
    "@ethersproject/address": "=5.7.0",
    "@ethersproject/bignumber": "=5.7.0",
    "@ethersproject/solidity": "=5.7.0",
    "@sushiswap/abi": "=0.0.1",
    "@sushiswap/amm": "=0.1.13",
    "@sushiswap/base-sdk": "=0.0.1",
    "@sushiswap/bentobox": "=1.0.1",
    "@sushiswap/chain": "=0.1.2",
    "@sushiswap/currency": "=0.1.2",
    "@sushiswap/database": "=0.0.1",
    "@sushiswap/math": "=0.1.0",
    "@sushiswap/router-config": "=2.0.0",
    "@sushiswap/tines": "=1.0.9",
    "@sushiswap/trident-core": "=1.0.8",
    "@sushiswap/v2-sdk": "=0.0.9",
    "@sushiswap/v3-sdk": "=1.0.10",
    "@types/lodash.flatmap": "^4.5.7",
    "@types/node": "18",
    "@types/node-fetch": "2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "date-fns": "2.29.3",
    "esbuild": "^0.19.4",
    "eslint": "8.43.0",
    "ethers": "5.7.2",
    "lodash.flatmap": "^4.5.0",
    "memoize-fs": "github:rouzwelt/memoize-fs#e5fcc9f6effc4ad087514372a53a49d380520ad5",
    "node-fetch": "2.6.6",
    "tiny-invariant": "1.3.1",
    "tiny-warning": "1.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "zod": "3.21.4"
  }
}
