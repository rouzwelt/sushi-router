{
  // "extends": "@sushiswap/typescript-config/node18.json",
  "include": ["src"],
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "outDir": "./dist",
    // "outFile": "./dist/index",
    "rootDir": "src",
    "skipLibCheck": true,
    "declaration": true,
    // "declarationMap": true,
    "sourceMap": false,
    "strict": false,
    "esModuleInterop": true,
    "emitDeclarationOnly": true,
  }
}
